<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/black"
    android:id="@+id/Screen6_root"
    >

<TableLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >
<TableRow
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >
	<TextView  
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content"
	    android:layout_column="0"
	    android:layout_weight="1"     
	    android:text=""
	    />
	 <TextView  
	    android:layout_width="wrap_content" 
	    android:layout_height="50dp" 
	    android:layout_column="1"
	    android:layout_weight="20"
	    android:textSize="24sp"
	    android:gravity="center_horizontal"
	    android:text="@string/screen6_caption"
	    />
	<TextView  
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content"
	    android:layout_column="2"
	    android:layout_weight="1"     
	    android:text=""
	    />
</TableRow>
</TableLayout>
    

<TableLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >
	<TableRow
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    >
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="0"
		    android:layout_weight="1"     
		    android:text=" Project Name:"
		    android:textSize="16sp"
		    />
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="1"
		    android:layout_weight="3"
		    android:id="@+id/Screen6_tvProjectName"   
		    android:text=""
		    android:textSize="16sp"
		    />
	</TableRow>
	<TableRow
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    >
			
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="0"
		    android:layout_weight="1"     
		    android:text=" Username:"
		    android:textSize="16sp"
		    />
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="1"
		    android:layout_weight="3"
		    android:id="@+id/Screen6_tvUsername"   
		    android:text=""
		    android:textSize="16sp"
		    />
	</TableRow>
	
	<TableRow
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    >
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="0"
		    android:layout_weight="1"     
		    android:text=" Start Time:"
		    android:textSize="16sp"
		    />
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="1"
		    android:layout_weight="3"
		    android:id="@+id/Screen6_tvStartTime"   
		    android:text=""
		    android:textSize="16sp"
		    />
	</TableRow>

	<TableRow
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    >
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="0"
		    android:layout_weight="1"     
		    android:text=" End Time:"
		    android:textSize="16sp"
		    />
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:layout_column="1"
		    android:layout_weight="3"
		    android:id="@+id/Screen6_tvEndTime"   
		    android:text=""
		    android:textSize="16sp"
		    />
	</TableRow>

	<TableRow
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    >
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="40dp"
		    android:layout_column="0"
		    android:layout_weight="1"     
		    android:text=" #Labels:"
		    android:textSize="16sp"
		    />
		<TextView  
		    android:layout_width="wrap_content" 
		    android:layout_height="40dp"
		    android:layout_column="1"
		    android:layout_weight="3"
		    android:id="@+id/Screen6_tvLabelNumber"   
		    android:text=""
		    android:textSize="16sp"
		    />
	</TableRow>
						
</TableLayout>


<TableLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="bottom"
    >
	<TableRow
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    >
		<Button android:layout_height="80dp"
				android:layout_width="0.0dip"
				android:id="@+id/btScreen6_Back"
				android:text="Back"
	    		android:layout_column="0"				
			    android:layout_weight="1"
			    android:textSize="20sp"				
				/>		
	    
	    <Button android:layout_height="80dp"
				android:layout_width="0.0dip"
				android:id="@+id/btScreen6_Upload"
	    		android:layout_column="1"
			    android:layout_weight="1"
				android:text="Upload Data"
				android:textSize="20sp"
				/>				    
	</TableRow>
</TableLayout>


</LinearLayout>

